{
  "name": "lenzhound-manager",
  "version": "0.0.0",
  "description": "Lenzhound settings and timelapse manager.",
  "main": "main.js",
  "scripts": {
    "build": "babel ./src -d ./lib",
    "package-win32-x64": "electron-packager . \"Lenzhound Leash\" --platform=win32 --arch=x64 --icon=\"content/icon.ico\" --overwrite --asar=true",
    "package-osx-x64": "electron-packager . \"Lenzhound Leash\" --platform=darwin --arch=x64 --icon=\"content/icon.icns\" --overwrite --asar=true",
    "post-install": "electron-rebuild -p",
    "start": "electron main.js dev"
  },
  "keywords": [
    "lenzhound",
    "timelapse"
  ],
  "author": "Doug Thayer",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "electron-packager": "^8.5.0",
    "electron-rebuild": "^1.3.0"
  },
  "dependencies": {
    "avrgirl-arduino": "^1.9.8",
    "babel-plugin-transform-react-jsx": "^6.22.0",
    "electron": "^1.4.15",
    "electron-rebuild": "^1.3.0",
    "event-emitter": "^0.3.4",
    "lodash": "^4.16.4",
    "material-ui": "^0.14.2",
    "promisify-node": "^0.4.0",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "serialport": "^4.0.3",
    "string-hash": "^1.1.0"
  }
}
